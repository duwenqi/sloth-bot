{
  "name": "sloth-bot",
  "version": "1.0.3",
  "description": "Slack Bot full of fun commands",
  "author": {
    "name": "Luigi Poole",
    "email": "luigipoole@outlook.com",
    "url": "https://github.com/luigiplr"
  },
  "contributors": [
    {
      "name": "Js41637",
      "email": "js41637@gmail.com",
      "url": "https://github.com/js41637"
    }
  ],
  "scripts": {
    "dev": "better-npm-run start-dev",
    "debug": "better-npm-run start-dev-inspector",
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "forever": "better-npm-run forever"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node -r babel-register ./index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-dev-inspector": {
      "command": "node --inspect -r babel-register ./index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "babel src --out-dir build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node ./build/sloth.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "forever": {
      "command": "forever start -a -o out.log -e err.log -m 1 index.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luigiplr/sloth-bot.git"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "main": "build/sloth.js",
  "license": "Beerware",
  "dependencies": {
    "@slack/client": "^3.8.1",
    "async": "^2.1.4",
    "caniuse-api": "^1.5.2",
    "cleverbot-node": "^0.2.5",
    "codepad": "^0.1.1",
    "createreadupdatedelete.js": "^0.8.0",
    "eightball": "^1.0.1",
    "giphy": "0.0.4",
    "insultgenerator": "0.0.2",
    "lodash": "^4.17.4",
    "mineping": "^0.1.0",
    "moment": "^2.17.1",
    "morse-node": "^0.1.1",
    "needle": "^1.4.3",
    "node-metainspector": "https://github.com/jaruba/node-metainspector/tarball/master",
    "node-uuid": "^1.4.7",
    "request": "^2.79.0",
    "scrape-mdn": "^1.0.2",
    "shakespeare-insult": "^1.0.0",
    "steamid": "^1.1.0",
    "striptags": "^2.1.1",
    "urban": "^0.3.1",
    "xml2js": "^0.4.17",
    "youtube-search": "^1.0.7"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "better-npm-run": "0.0.13",
    "piping": "^1.0.0-rc.4"
  }
}
